USES DOS;
VAR X:INTEGER;
	 A,B,C:BYTE;
	 F:TEXT;
	 T:ARRAY[1..160] OF BYTE;
	 S:STRING;
BEGIN
	RANDOMIZE;
	FOR X:=1 TO 160 DO T[X]:=X-1;
	FOR X:=1 TO 10000 DO
	BEGIN
		A:=RANDOM(160)+1;
		B:=RANDOM(160)+1;
		C:=T[B];
		T[B]:=T[A];
		T[A]:=C;
	END;
	ASSIGN(F,'CUT.TXT');
	REWRITE(F);
	FOR X:=1 TO 160 DO
	BEGIN
		STR(T[X],S);
		IF (X MOD 20 <> 0) THEN WRITE(F,S+',')
								 ELSE WRITELN(F,S);
	END;
	CLOSE(F);
END.